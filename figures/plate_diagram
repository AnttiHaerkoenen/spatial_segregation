	"σ" [label="σ
~
HalfNormal" shape=ellipse]
	"α" [label="α
~
Normal" shape=ellipse]
	subgraph cluster4 {
		w [label="w
~
Normal" shape=ellipse]
		label=4 labeljust=r labelloc=b style=rounded
	}
	subgraph cluster8 {
		S [label="S
~
Normal" shape=ellipse style=filled]
		"μ" [label="μ
~
Deterministic" shape=box]
		label=8 labeljust=r labelloc=b style=rounded
	}
	"α" -> "μ"
	w -> "μ"
	"σ" -> S
	"μ" -> S
}
